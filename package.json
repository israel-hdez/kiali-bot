{
  "name": "kiali-bot",
  "version": "0.2.0",
  "description": "A bot to manage the workflow for Kiali Project",
  "author": "Kiali Mantainers Team <kiali-dev@googlegroups.com> (https://kiali.io)",
  "license": "SEE LICENSE IN LICENSE",
  "repository": "https://github.com/kiali/kiali-bot.git",
  "homepage": "https://github.com/kiali/kiali-bot",
  "bugs": "https://github.com/kiali/kiali-bot/issues",
  "private": true,
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "files": [
    "lib/**/*"
  ],
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "nodemon --exec \"(npm run build || exit 1) && (npm start || exit 1)\"",
    "start": "probot run ./lib/index.js",
    "lint": "eslint --fix '{src,test}/**/*.ts'",
    "test": "env $(cat .env.tests) jest && eslint '{src,test}/**/*.ts'",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "@octokit/rest": "^16.21.0",
    "@types/nock": "^9.3.0",
    "handlebars": "^4.3.0",
    "moment": "^2.24.0",
    "nock": "^10.0.0",
    "node-cmd": "^3.0.0",
    "node-mailjet": "^3.3.1",
    "probot": "^9.2.13",
    "probot-config": "^1.1.0",
    "tempfile": "^2.0.0"
  },
  "devDependencies": {
    "@octokit/webhooks": "^5.3.3",
    "@types/handlebars": "^4.0.39",
    "@types/jest": "^24.0.0",
    "@types/node": "^11.9.0",
    "@types/node-mailjet": "^3.3.0",
    "@types/tempfile": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "^1.9.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.8.0",
    "nodemon": "^1.17.2",
    "prettier": "^1.17.1",
    "smee-client": "^1.0.2",
    "ts-jest": "^24.0.0",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
